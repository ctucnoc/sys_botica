	<queryString language="plsql">
		<![CDATA[select
pr.name,
round(ds.saleprice::numeric,2) as saleprice,
cast(sum(ds.saleamount) as integer) as saleamount,
round((ds.saleprice*cast(sum(ds.saleamount) as integer))::numeric,2) as subtotal,
crce.sp_total_sale($P{p_dateFrom},$P{p_dateTo} ) as total
from
crce.adm_sale sa,
crce.adm_dt_sale ds,
crce.adm_dt_transfer dt,
crce.adm_dt_entry_product ep,
crce.adm_product pr
where
sa.id = ds.idsale and
ds.iddttransfer = dt.id and
dt.iddtentryproduct = ep.id and
ep.idproduct = pr.id and
sa.state = '1' and
ds.state = '1' and
cast(cast(sa.datecreated as date) as varchar) between $P{p_dateFrom}  and $P{p_dateTo} 
group by pr.name,ds.saleprice
order by pr.name desc;]]>
	</queryString>
